  </cron>

    
  <cron>
    <url>/wancostresumming1h</url>
    <description>creates 2nd averaged table.</description>
    <schedule>every 1 hours</schedule> 
  </cron>
  

  <cron>
    <url>/wancostresumming3h</url>
    <description>creates 3rd averaged table.</description>
    <schedule>every 3 hours</schedule> 
  </cron>
  

  <cron>
    <url>/wancostresumming6h</url>
    <description>creates 4th averaged table.</description>
    <schedule>every 6 hours</schedule> 
  </cron>
  
 
  <cron>
    <url>/wancostresumming12h</url>
    <description>creates 5th averaged table.</description>
    <schedule>every 12 hours</schedule> 
  </cron>

  <cron>
    <url>/wancostresumming24h</url>
    <description>creates 6th averaged table.</description>
    <schedule>every 24 hours</schedule> 
  </cron>
 
  <cron>
    <url>/UpdateFailover</url>
    <description>fetch failovers.</description>
    <schedule>every 4 hours</schedule> 
  </cron>
  
