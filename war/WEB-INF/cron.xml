<cronentries>
  <cron>
    <url>/UpdateFromAGIS</url>
    <description>Updates site coordinates every night.</description>
    <!-- <schedule>every day 00:30</schedule> -->
    <schedule>every 2 hours</schedule>
  </cron>
  
  <cron>
    <url>/wancostdelete</url>
    <description>deletes old measurements.</description>
    <schedule>every 3 hours</schedule> 
  </cron>
  
  <cron>
    <url>/wancostresumming</url>
    <description>creates first averaged table.</description>
